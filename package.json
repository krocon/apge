{
  "name": "apge",
  "version": "0.2.0",
  "description": "Angular Project Generator",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-g-local": "npm uninstall -g apge & npm install -g ./",
    "git-push": "git commit -am ':-)' & git push",
    "npm-publish": "npm version minor & npm publish",
    "publish": "npm run git-push & npm run npm-publish"
  },
  "main": "lib/cmd-generate-new.js",
  "bin": {
    "apge": "./bin/apge.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krocon/apge.git"
  },
  "keywords": [
    "angular",
    "generator",
    "project",
    "seed"
  ],
  "files": [
    "bin/apge.js",
    "lib/cmd-generate-new.js",
    "lib/cmd-update.js",
    "!test"
  ],
  "readmeFilename": "README.md",
  "author": {
    "name": "Marc Kronberg",
    "email": "krocon@gmail.com",
    "url": "https://gui.expert/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krocon/apg/issues"
  },
  "homepage": "https://github.com/krocon/apg#readme",
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^5.0.0",
    "fs-extra": "^9.0.0",
    "progress": "^2.0.3",
    "prompts": "^2.3.2",
    "replace-in-file": "^5.0.2"
  }
}
